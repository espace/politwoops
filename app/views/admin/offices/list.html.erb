<div id="admin-list">
<link href="/stylesheets/admin.css" media="screen" rel="stylesheet" type="text/css" />
<h3><%= t :list_of_offices, :scope => [:politwoops,:admin] %>&nbsp; <span style="font-size: .8em;"><em><a href="/admin/offices/add/"><%= t :add_new, :scope => [:politwoops,:form] %> &raquo;</a></em></span></h3>

<ul style="margin-top:20px;">
<% @offices.each do |o| %>
    <li><a href="/admin/offices/<%=o.id%>">
    <%= o.title %> (<%= o.abbreviation %>)
        </a>
    </li>
<% end %>
</ul>
</div>
